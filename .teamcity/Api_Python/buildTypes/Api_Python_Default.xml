<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="5aeeb013-b1bc-40d7-afda-c21fab2cee92" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2025.3/project-config.xsd">
  <name>Default</name>
  <description>Builds only release artifacts</description>
  <settings>
    <options>
      <option name="buildConfigurationType" value="COMPOSITE" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <vcs-settings>
      <vcs-entry-ref root-id="QdbApiPython" />
    </vcs-settings>
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_426" sourceBuildTypeId="Api_Python_Release_Composite" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="**" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="Api_Python_Release_Composite">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
  </settings>
</build-type>

