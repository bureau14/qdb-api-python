<?xml version="1.0" encoding="UTF-8"?>
<template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Build</name>
  <settings>
    <options>
      <option name="artifactRules" value="dist/quasardb*.egg&#xA;dist/quasardb*.whl&#xA;insecure/log =&gt; logs/insecure&#xA;secure/log =&gt; logs/secure" />
      <option name="executionTimeoutMin" value="120" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <parameters>
      <param name="JUNIT_XML_FILE" value="build/test/pytest.xml" />
      <param name="archive.ext" value="tar.zst" />
      <param name="dependencies.artifact.rules"><![CDATA[*-c-api.%archive.ext%!**/*=>qdb
*-utils.%archive.ext%!**/*=>qdb
*-server.%archive.ext%!**/*=>qdb]]></param>
      <param name="env.PYTHON_CMD" value="%python_executable%" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_321" name="" type="_placeholder_">
        <parameters>
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_102" name="Start services" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="bash" />
          <param name="command.parameters" value="scripts/tests/setup/start-services.sh" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="" />
        </parameters>
      </runner>
      <runner id="RUNNER_48" name="Test" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[export QDB_TESTS_ENABLED=ON
%python_executable% -m pip install --user -r dev-requirements.txt
%python_executable% setup.py test --addopts "--junitxml=%JUNIT_XML_FILE%"
export QDB_TESTS_ENABLED=OFF]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_143" name="Stop services" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="bash" />
          <param name="command.parameters" value="scripts/tests/setup/stop-services.sh" />
          <param name="teamcity.step.mode" value="execute_always" />
          <param name="use.custom.script" value="" />
        </parameters>
      </runner>
      <runner id="RUNNER_112" name="Build" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="bash" />
          <param name="command.parameters" value="scripts/teamcity/10.build.sh" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="" />
        </parameters>
      </runner>
      <runner id="RUNNER_98" name="Smoke test" type="simpleRunner">
        <parameters>
          <param name="script.content" value="./smoke-test.sh ../../dist/quasardb*.whl" />
          <param name="teamcity.build.workingDir" value="scripts/test/" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="QdbApiPython" />
    </vcs-settings>
    <requirements>
      <ver-no-less-than id="RQ_26" name="system.cmake.version" value="2.8" />
      <equals id="RQ_10" name="teamcity.agent.jvm.os.name" value="Mac OS X" />
      <exists id="RQ_13" name="system.ninja.version" />
      <equals id="RQ_6" name="teamcity.agent.jvm.os.arch" value="%os_arch%" />
    </requirements>
    <build-triggers />
      <extension id="perfmon" type="perfmon">
        <parameters>
          <param name="teamcity.perfmon.feature.enabled" value="true" />
        </parameters>
      </extension>
    </build-extensions>
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_423" sourceBuildTypeId="Quasardb_Artifacts_Release_MacOS_AArch64_Composite" cleanDestination="true">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="*-darwin-64bit-arm64-c-api.tar.zst!**/*=&gt;qdb&#xD;&#xA;*-darwin-64bit-arm64-utils.tar.zst!**/*=&gt;qdb&#xD;&#xA;*-darwin-64bit-arm64-server.tar.zst!**/*=&gt;qdb" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="Quasardb_Artifacts_Release_MacOS_AArch64_Composite">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</template>
