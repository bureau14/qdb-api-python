<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Python 3.6</name>
  <description />
  <settings ref="Api_Python_Windows_Win64_Build">
    <disabled-settings>
      <setting-ref ref="RQ_153" />
    </disabled-settings>
    <parameters>
      <param name="PYTHON_CMD" value="%system.python36-64.exe%" />
      <param name="env.PYTHON_CMD" value="%system.python36-64.exe%" />
      <param name="env.PYTHON_EXECUTABLE" value="%system.python36-64.exe%" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_48" name="Test" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="bash" />
          <param name="command.parameters" value="scripts/teamcity/20.test.sh" />
          <param name="script.content"><![CDATA[%PYTHON_CMD% -m venv .env/
source .env/bin/activate
%PYTHON_CMD% -m pip install --user -r dev-requirements.txt
%PYTHON_CMD% setup.py test  --addopts "--junitxml=%JUNIT_XML_FILE%"]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements>
      <equals id="RQ_153" name="system.python36.cpu" value="x64" />
    </requirements>
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

